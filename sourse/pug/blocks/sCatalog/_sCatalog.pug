mixin sCatalog(data)
	// start sCatalog
	+b.SECTION.sCatalog.section#sCatalog&attributes(attributes)
		.container
			+b.section-title
				h1 Каталог
				
			.row
				mixin item(i,t)
					+e.col.col-6.col-md-4.col-lg-3.col-xxl-2
						+e.item(href="#")
							+e.imgWrap 
								+e.bg 
									+pic('bg-fon.png')
								+e.mainImg
									+pic(`catalog-${i}`,'','')
							+e.itemTitle= t
				+item('1','Готовые смеси для выпечки')
				+item('2','Блюда удобного приготовления')
				+item('3','Правильное питание')
				+item('4','Мюсли, хлопья, каши')
				+item('5','Крупы')
				+item('6','Бобовые')
				+item('7','Мука, крахмал, панировочные сухари')
				+item('8','Наборы для приготовления')
				+item('9','Продукты в офис и в дорогу')
				+item('10','Макаронные изделия')
				+item('11','Сахар, соль, сода, лимонная кислота')
				+item('12','Сухофрукты, орехи и сухие грибы')
				+item('13','Кондитерское сырьё, халва, чай')
				+item('14','Молочные продукты')
				+item('15','Мясные консервы')
				+item('16','Растительное масло')
				+item('17','HoReCa')

	// end sCatalog